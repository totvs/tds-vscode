	},
	"Method": {
		"prefix": "Method",
		"body": [
			"/*/{Protheus.doc} ${1:methodName}",
			"\t(long_description)",
			"\t@author ${2:user}",
			"\t@since ${3:$CURRENT_DATE/$CURRENT_MONTH/$CURRENT_YEAR}",
