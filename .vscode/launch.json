{
	"version": "0.2.0",
	"configurations": [
		{
			"args": [
				"task"
			],
			"name": "Gulp task",
			"program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node"
		},
		{
			"type": "extensionHost",
			"request": "launch",
			"name": "Extension",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}"
			],
			"outFiles": [
				"${workspaceFolder}/out/**/*.js"
			],
			"preLaunchTask": "npm: compile",
			"env": {
				"TOTVS_LS_LOG_FILE": "./TOTVS_LS_LOG_FILE.log"
			}
		},
		{
			"type": "extensionHost",
			"request": "launch",
			"name": "Extension WithoutCompile",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}"
			],
			"outFiles": [
				"${workspaceFolder}/out/**/*.js"
			],
			"preLaunchTask": "",
			"env": {
				"TOTVS_LS_LOG_FILE": "./TOTVS_LS_LOG_FILE.log"
			}
		},
		{
			"type": "totvs_language_debug",
			"request": "launch",
			"cwb": "${workspaceRoot}",
			"name": "teste",
			"smartclientBin": "D:\\Totvs\\SmartClients\\SC-20.3.0.x\\smartclient.exe",
			"program": "u_exgrid",
			"programArguments": [],
			"enableMultiThread": false,
			"enableProfile": false,
			"isMultiSession": true,
			"isAccessibilityMode": false,
			"doNotShowSplash": false,
			"openglMode": false,
			"dpiMode": false,
			"olddpiMode": false,
			"language": "2",
			"ignoreFiles": true,
			"enableTableSync": true
		}
	]
}