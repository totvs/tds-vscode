				"mac": "cmd+f9",
				"when": "editorTextFocus"
			},
			{
				"command": "totvs-developer-studio.rebuild.file",
				"key": "ctrl+shift+f9",
				"mac": "cmd+shift+f9",
				"when": "editorTextFocus"
			},
			{
				"command": "totvs-developer-studio.build.openEditors",
				"key": "ctrl+f10",
				"mac": "cmd+f10"
			},
			{
				"command": "totvs-developer-studio.rebuild.openEditors",
				"key": "ctrl+shift+f10",
				"mac": "cmd+shift+f10"
			},
			{
				"command": "totvs-developer-studio.toggle.autocomplete.behavior",
				"key": "ctrl+alt+space",
				"when": "editorTextFocus"
			}
		],
