{
	"name": "tds-vscode",
	"displayName": "Totvs Developer Studio for VSCode (AdvPL, TLPP e 4GL)",
	"description": "%tds.package.description%",
	"version": "1.0.2",
	"publisher": "TOTVS",
	"author": {
		"name": "TOTVS",
		"email": "tecnologia@totvs.com.br"
	},
	"license": "MIT",
	"keywords": [
		"multi-root ready",
		"advpl",
		"tlpp",
		"4gl",
		"tds",
		"totvs"
	],
	"engines": {
		"vscode": "^1.29.0",
		"node": "^8.9.3"
	},
	"icon": "imagens/totvs-developer-studio.png",
	"categories": [
		"Programming Languages",
		"Snippets",
		"Linters",
		"Debuggers",
		"Formatters"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/totvs/tds-vscode.git"
	},
	"bugs": {
		"url": "https://github.com/totvs/tds-vscode.git"
	},
	"scripts": {
		"prepare": "tsc -p ./src",
		"compile": "npm-run-all compile:*",
		"watch": "npm-run-all -p watch:*",
		"compile:extension": "tsc -p ./src",
		"compile:views": "webpack --mode development",
		"watch:extension": "tsc -w -p ./src",
		"watch:views": "webpack --watch --mode development",
		"tslint": "tslint ./src/**/*.ts",
		"test": "node node_modules/vscode/bin/test",
		"package": "npm run compile && vsce package",
		"publish": "npm run compile && vsce publish"
	},
	"dependencies": {
		"@material-ui/core": "^4.9.5",
		"@material-ui/icons": "^4.9.1",
		"@material-ui/lab": "^4.0.0-alpha.45",
		"@totvs/tds-da": "^1.1.4",
		"@totvs/tds-ls": "^1.1.8",
		"@types/jszip": "^3.1.7",
		"@types/node": "13.7.1",
		"@types/react": "^16.9.23",
		"@types/vscode": "^1.44.0",
		"cheerio": "^1.0.0-rc.3",
		"core-js": "^3.6.4",
		"ini": "^1.3.5",
		"jszip": "^3.4.0",
		"material-table": "^1.57.2",
		"react": "^16.13.0",
		"react-dom": "^16.13.0",
		"strip-json-comments": "^2.0.1",
		"template-literal": "^1.0.3",
		"tmp": "^0.1.0",
		"typescript": "3.7.5",
		"vscode-debugadapter": "1.32.1",
		"vscode-languageclient": "^4.4.2",
		"vscode-nls": "^4.1.2",
		"vscode-nls-dev": "^3.3.1"
	},
	"devDependencies": {
		"@types/react-dom": "^16.9.5",
		"cleanup": "^0.3.0",
		"css-loader": "^3.5.3",
		"del": "^4.1.1",
		"gulp": "^4.0.2",
		"gulp-sourcemaps": "^2.6.5",
		"gulp-tslint": "^8.1.4",
		"gulp-typescript": "^5.0.1",
		"gulp-util": "^3.0.8",
		"npm-run-all": "^4.1.5",
		"style-loader": "^1.2.1",
		"ts-loader": "^6.2.2",
		"tslint": "5.11.0",
		"vsce": "^1.74.0",
		"webpack": "^4.42.0",
		"webpack-cli": "^3.3.10"
	},
	"main": "./out/extension",
	"activationEvents": [
		"onLanguage:advpl",
		"onCommand:totvs-developer-studio.getProgramArguments",
		"onCommand:totvs-developer-studio.getProgramName",
		"onCommand:totvs-developer-studio.build.file",
		"onCommand:totvs-developer-studio.rebuild.file",
		"onCommand:totvs-developer-studio.build.workspace",
		"onCommand:totvs-developer-studio.rebuild.workspace",
		"onCommand:totvs-developer-studio.build.openEditors",
		"onCommand:totvs-developer-studio.rebuild.openEditors",
		"onCommand:totvs-developer-studio.delete.file.fromRPO",
		"onCommand:totvs-developer-studio.configure.launcher",
		"onCommand:totvs-developer-studio.inspectorFunctions",
		"onCommand:totvs-developer-studio.inspectorObjects",
		"onCommand:totvs-developer-studio.defragRPO",
		"onCommand:totvs-developer-studio.rpoCheckIntegrity",
		"onCommand:totvs-developer-studio.delete.file.fromRPO",
		"onCommand:totvs-developer-studio.getDAP",
		"onCommand:totvs-developer-studio.connect",
		"onCommand:totvs-developer-studio.reconnect",
		"onCommand:totvs-developer-studio.serverSelection",
		"onCommand:totvs-developer-studio.disconnect",
		"onCommand:totvs-developer-studio.add",
		"onCommand:totvs-developer-studio.config",
		"onCommand:totvs-developer-studio.selectenv",
		"onCommand:totvs-developer-studio.delete",
		"onCommand:totvs-developer-studio.patchGenerate.fromRPO",
		"onCommand:totvs-developer-studio.patchGenerate.fromFolder",
		"onCommand:totvs-developer-studio.patchApply",
		"onCommand:totvs-developer-studio.patchApply.fromFile",
		"onCommand:totvs-developer-studio.patchInfos",
		"onCommand:totvs-developer-studio.patchInfos.fromFile",
		"onCommand:totvs-developer-studio.welcomePage",
		"onCommand:totvs-developer-studio.include",
		"onCommand:totvs-developer-studio.ws.show",
		"onCommand:totvs-developer-studio.logger.on",
		"onCommand:totvs-developer-studio.logger.off",
		"onCommand:totvs-developer-studio.compile.key",
		"onView:totvs_server",
		"workspaceContains:.tds",
		"workspaceContains:*.prw",
		"workspaceContains:*.ppo",
		"workspaceContains:*.prx",
		"workspaceContains:*.aph",
		"workspaceContains:*.ppx",
		"workspaceContains:*.ppp",
		"workspaceContains:*.tlpp",
		"workspaceContains:*.ch",
		"workspaceContains:*.4gl",
		"workspaceContains:*.per",
		"workspaceContains:*.ptm",
		"workspaceContains:*.upd",
		"workspaceContains:*.pak",
		"workspaceContains:servers.json"
	],
	"contributes": {
		"colors": [
			{
				"id": "terminal.ansiBlack",
				"description": "Cor de texto apresentado no console de depuração.",
				"defaults": {
					"dark": "#CCCCCC",
					"light": "#555555",
					"highContrast": "#ffffff"
				}
			},
			{
				"id": "terminal.ansiGreen",
				"description": "Cor da identifcação do nível de mensagem 'Informação'.",
				"defaults": {
					"dark": "#5aa85a",
					"light": "#5aa85a",
					"highContrast": "#5aa85a"
				}
			},
			{
				"id": "terminal.ansiYellow",
				"description": "Cor da identifcação do nível de mensagem 'Aviso'.",
				"defaults": {
					"dark": "#d8d53c",
					"light": "#d8d53c",
					"highContrast": "#d8d53c"
				}
			},
			{
				"id": "terminal.ansiRed",
				"description": "Cor da identifcação do nível de mensagem 'Erro'.",
				"defaults": {
					"dark": "#eb1313",
					"light": "#eb1313",
					"highContrast": "#eb1313"
				}
			},
			{
				"id": "terminal.ansiCyan",
				"description": "Cor da identifcação da hora do evento.",
				"defaults": {
					"dark": "#11c9cf",
					"light": "#11c9cf",
					"highContrast": "#11c9cf"
				}
			}
		],
		"languages": [
			{
				"id": "advpl",
				"aliases": [
					"AdvPL",
					"advpl"
				],
				"extensions": [
					".ch",
					".prw",
					".prg",
					".prx",
					".ppx",
					".ppp",
					".tlpp",
					".aph",
					".ahu",
					".apl",
					".apw",
					".4gl",
					".per"
				],
				"configuration": "./language-configuration.json"
			},
			{
				"id": "totvs_patch",
				"aliases": [
					"Patch",
					"totvs_patch"
				],
				"extensions": [
					".ptm",
					".upd",
					".pak"
				]
			}
		],
		"grammars": [
			{
				"language": "advpl",
				"scopeName": "source.advpl",
				"path": "./syntaxes/advpl_language.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "advpl",
				"path": "./snippets/snippets.json"
			}
		],
		"configurationDefaults": {},
		"configuration": {
			"type": "object",
			"title": "AdvPL",
			"properties": {
				"totvsLanguageServer.launch.args": {
					"type": "array",
					"default": [],
					"description": "%tds.package.args.for.lsp%"
				},
				"totvsLanguageServer.folder.enableExtensionsFilter": {
					"scope": "window",
					"type": "boolean",
					"default": true,
					"description": "%tds.package.folder.enable.extensions.filter%"
				},
				"totvsLanguageServer.folder.extensionsAllowed": {
					"scope": "resource",
					"type": "array",
					"default": [
						".PRW",
						".PRX",
						".PRG",
						".PPX",
						".PPP",
						".TLPP",
						".APW",
						".APH",
						".APL",
						".AHU",
						".TRES",
						".PNG",
						".BMP",
						".RES",
						".4GL",
						".PER"
					],
					"description": "%tds.package.folder.extensions.allowed%"
				},
				"totvsLanguageServer.trace.server": {
					"scope": "window",
					"type": "string",
					"enum": [
						"off",
						"messages",
						"verbose"
					],
					"default": "off",
					"description": "%tds.package.trace.vscode.server%"
				},
				"totvsLanguageServer.welcomePage": {
					"scope": "window",
					"type": "boolean",
					"default": true,
					"description": "%tds.package.show.welcome.first.initialize%"
				},
				"totvsLanguageServer.askEncodingChange": {
					"scope": "window",
					"type": "boolean",
					"default": true,
					"description": "%tds.package.show.ask.encoding%"
				},
				"totvsLanguageServer.askCompileResult": {
					"scope": "window",
					"type": "boolean",
					"default": true,
					"description": "%tds.package.show.ask.compile.result%"
				},
				"totvsLanguageServer.clearConsoleBeforeCompile": {
					"type": "boolean",
					"default": false,
					"description": "%tds.package.clear.console.before.compile%"
				},
				"totvsLanguageServer.showConsoleOnCompile": {
					"type": "boolean",
					"default": true,
					"description": "%tds.package.show.console.on.compile%"
				},
				"totvsLanguageServer.reconnectLastServer": {
					"type": "boolean",
					"default": true,
					"description": "%tds.package.reconnectLastServer%"
				},
				"totvsLanguageServer.compilation.generatePpoFile": {
					"type": "boolean",
					"default": false,
					"description": "%tds.package.generate.ppo%"
				},
				"totvsLanguageServer.compilation.showPreCompiler": {
					"type": "boolean",
					"default": false,
					"description": "%tds.package.show.precompile%"
				},
				"totvsLanguageServer.web.navigator": {
					"type": "string",
					"default": "",
					"description": "%tds.package.web.navigator.debug%"
				},
				"totvsLanguageServer.editor.show.notification": {
					"scope": "window",
					"type": "string",
					"enum": [
						"none",
						"only errors",
						"errors and warnings",
						"errors warnings and infos",
						"all"
					],
					"enumDescriptions": [
						"%tds.package.editor.notification.show.none%",
						"%tds.package.editor.notification.show.only_errors%",
						"%tds.package.editor.notification.show.errors_and_warnings%",
						"%tds.package.editor.notification.show.errors_warnings_and_infos%",
						"%tds.package.editor.notification.show.all%"
					],
					"default": "errors warnings and infos",
					"description": "%tds.package.editor.notification.show%"
				},
				"totvsLanguageServer.editor.toggle.autocomplete": {
					"scope": "window",
					"type": "string",
					"enum": [
						"Basic",
						"LS"
					],
					"enumDescriptions": [
						"%tds.package.editor.toggle.autocomplete.basic%",
						"%tds.package.editor.toggle.autocomplete.ls%"
					],
					"default": "LS",
					"description": "%tds.package.editor.toggle.autocomplete%"
				}
			}
		},
