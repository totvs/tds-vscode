			}
		],
		"debuggers": [
			{
				"type": "totvs_language_debug",
				"label": "%tds.package.totvs_language_debug%",
				"configurationAttributes": {
					"launch": {
						"required": [
							"program",
							"smartclientBin"
						],
						"properties": {
							"program": {
								"type": "string",
